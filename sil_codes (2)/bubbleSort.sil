decl
	integer a,b[100];
enddecl

integer main(){

decl
	integer n,i,c,d,t ;
enddecl

begin

read (n);

i = 1;
while ( i <= n) do
read ( b[i-1] );
i = i + 1;
endwhile;

c=0;
while ( c < n - 1) do
    d=0;
    while ( d < n - c - 1) do
      if (b[d] > b[d+1]) then
        t      = b[d];
        b[d]   = b[d+1];
        b[d+1] = t;
      endif;
    d=d+1;	
    endwhile;
    c=c+1;	
endwhile;

i = 1;

while ( i <= n) do
	write ( b[i-1] );
	i = i + 1;
endwhile;

return 1;

end

}