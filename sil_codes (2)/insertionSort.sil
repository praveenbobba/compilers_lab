decl
	integer a,b[100];
enddecl

integer main( ){

decl
	integer n,i,c,d,t;
enddecl

begin

read (n);

i = 1;
while ( i <= n) do
	read ( b[i-1] );
	i = i + 1;
endwhile;

c=1;

while ( c <= n - 1) do
    d = c;
    while ( d > 0 && b[d] < b[d-1]) do
      t      = b[d];
      b[d]   = b[d-1];
      b[d-1] = t;
      d=d-1;
    endwhile;
    c=c+1;	
endwhile;

i = 1;

while ( i <= n) do
	write ( b[i-1] );
	i = i + 1;
endwhile;

return 1;

end
}
