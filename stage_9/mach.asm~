START
MOV SP, -1
MOV BP, SP
PUSH R0
CALL MAIN
POP R0
HALT
Lf:
PUSH BP
MOV BP, SP
MOV R1, 0
MOV R0, SP
ADD R0, R1
MOV SP, R0
MOV R2, -3
MOV R1, BP
ADD R1, R2
MOV R0, [R1]
MOV R1, 1
SUB R0, R1
MOV R2, -3
MOV R1, BP
ADD R1, R2
MOV [R1], R0
MOV R2, -3
MOV R1, BP
ADD R1, R2
MOV R0, [R1]
OUT R0
MOV R2, -3
MOV R1, BP
ADD R1, R2
MOV R0, [R1]
MOV R1, BP
MOV R2, 2
SUB R1, R2
MOV [R1], R0
MOV SP, BP
POP BP
RET
MAIN:
PUSH BP
MOV BP, SP
MOV R1, 2
MOV R0, SP
ADD R0, R1
MOV SP, R0
MOV R0, 0
MOV R1, 5
MOV R2, 1
EQ R1, R2
JNZ R1, L2
L1:
INR R0
PUSH R0
PUSH R1
PUSH R2
CALL Lf
POP R2
POP R1
POP R0
MOV R1, R2
MOV R2, 1
EQ R2, R1
JZ R2, L1
L2:
JMP L3
L3:
MOV R2, 2
MOV R1, BP
ADD R1, R2
MOV [R1], R0
MOV R0, 0
MOV R1, BP
MOV R2, 2
SUB R1, R2
MOV [R1], R0
MOV SP, BP
POP BP
RET
