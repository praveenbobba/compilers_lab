decl
	integer a[2];
	integer swap(integer &n, &x);
enddecl

integer swap(integer &n)
{
	decl
		integer tmp;
	enddecl

	begin
		tmp = x;
		x = n;
		n = tmp;
		return true;
	end
}

integer main()
{
	decl
		integer ret;
	enddecl

	begin
		read(a[0]);
		read(a[1]);
		ret = swap(a[0], a[1]);
		write(a[0]);
		write(a[1]);
		return 0;
	end
}
