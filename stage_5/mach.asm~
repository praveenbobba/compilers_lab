START
MOV R0, 10
MOV [0], R0
MOV R0, 0
MOV [8], R0
L1:
MOV R0, [8]
MOV R1, [0]
LT R0, R1
JZ R0, L2
MOV R0, [8]
OUT R0
MOV R0, [8]
MOV R1, 1
ADD R0, R1
MOV [8], R0
JMP L1
L2:
IN R0
MOV [2], R0
MOV R0, [0]
MOV R1, [2]
EQ R0, R1
JZ R0, L3
MOV R0, [0]
OUT R0
JMP L4
L3:
MOV R0, [2]
OUT R0
MOV R0, [0]
OUT R0
L4:
HALT
